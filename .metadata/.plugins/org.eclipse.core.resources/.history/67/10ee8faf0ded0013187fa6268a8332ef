package com.example.weatherwithfriends;

public class Friend {
	private String name;
	private String city;
	private String state;
	private String country;
	
	Friend(String n, String c, String s, String co) {
		this.name = n;
		this.city = c;
		this.state = s;
		this.country = co;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String s) {
		this.name = s;
	}
	
	public String getCity() {
		return city;
	}
	
	
	
}
