package com.example.weatherwithfriends;

import java.net.URL;

public class WeatherInfo  {
	private String iconurl;
	private String forecasttext;
	
	public WeatherInfo(String i, String f){
		this.iconurl = i;
		this.forecasttext = f;
		
	}
	
	public URL getURL(){
		URL returnURL = null;
		returnURL = new URL(iconurl);
		return returnURL;
	}
}
